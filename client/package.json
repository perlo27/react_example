{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "immutable": "^3.8.1",
    "jquery": "^3.2.1",
    "loaders.css": "^0.1.2",
    "normalize.css": "^5.0.0",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-addons-transition-group": "^15.4.2",
    "react-day-picker": "^2.5.0",
    "react-dom": "^15.4.2",
